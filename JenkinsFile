    agent any

    stages {
        stage('Checkout') {
            steps {
                git branch: 'teste', url: 'https://github.com/GrrriiiM/DeOlho.git'
            }
        }
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}