    "version": "0.2.0",
    "configurations": [
        {
            "name": "Scheduler Dashboard",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/DeOlho.Scheduler.Dashboard/bin/Debug/netcoreapp2.1/DeOlho.Scheduler.Dashboard.dll",
            "args": [],
            "cwd": "${workspaceFolder}/DeOlho.Scheduler.Dashboard",
            "stopAtEntry": false,
            "launchBrowser": {
                "enabled": true
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/DeOlho.Scheduler.Dashboard/Views"
            }
        },
        {
            "name": "Scheduler Console",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/DeOlho.Scheduler.Console/bin/Debug/netcoreapp2.1/DeOlho.Scheduler.Console.dll",
            "args": [],
            "cwd": "${workspaceFolder}/DeOlho.Scheduler.Console/bin/Debug/netcoreapp2.1",
            "console": "externalTerminal",
            "stopAtEntry": false
        }
    ]
}